{
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "Node",
    "target": "es6",
    "sourceMap": true,
    "declaration": true,
    "jsx": "react",
    "esModuleInterop": true,
    "composite": true,
    "tsBuildInfoFile": "./dist/tsbuildinfo",
    "baseUrl": ".",
    "paths": {
      "@mono/domain/*": ["../domain/dist/*"]
    }
  },
  "include": ["src"],
  "references": [
    {
      "path": "../domain"
    }
  ]
}
